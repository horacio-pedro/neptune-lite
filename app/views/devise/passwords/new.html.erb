<section id="login-section" class="sign-up">
  <div class="container">
    <div class="login-container">
      <%# <span class="side-nav"></span> %>
      <a class="logo" href="#">
        <%= image_tag "logo.png", :size => "260x69" %>
      </a>
      <h2>Resetar Password?</h2>
      <div class="notice"><%= devise_error_messages! %></div>
      <%= form_for(resource, :as => resource_name, :url => password_path(resource_name), :html => {:method => :post,class: 'col s12', id: 'send_password'}) do |f| %>
          <%#= devise_error_messages! %>
          <%#= form_for(resource, :as => resource_name, :url => session_path(resource_name)) do |f| %>
          <div class='input-field'>
            <%= f.text_field :email, :id => "email", :class => 'sign_up user_email validate' %>
            <label for='email'>Email</label>
          </div>
          <div class="btn-content">
            <%= f.button "Enviar Instruções",:type=>'submit', :class => "btn_large submit col s4 btn waves-effect",:name=>'btn_login' %>
          </div>
      <% end %>
      <p class='col s12'>
        <%= link_to 'www.neptune.ao', 'http://neptune.ao/', target: '_blank' %>.
        powered by: <%= link_to 'conexoesinfinito.com', 'https://www.conexoesinfinito.com/', target: '_blank' %>
      </p>
      <a  href="<%=new_user_session_path() %>" class="btn-login-add btn-floating btn-large waves-effect forgot "><i class="material-icons close">close</i></a>
    </div>
  </div>
</section>
